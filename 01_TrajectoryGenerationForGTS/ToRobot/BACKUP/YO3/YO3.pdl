	pth1.NODE[31].$MAIN_POS :=POS(-1050.0,683.74,1650.0,0.0,0.0,0.0,' ')
	pth1.NODE[32].$MAIN_POS :=POS(-1050.0,25.00, 1650.0,0.0,0.0,0.0,' ')

	pth1.NODE[1].$MOVE_TYPE := LINEAR



	pth1.NODE[32].$MOVE_TYPE := LINEAR




	FOR int_var := 2 TO 16 DO



		pth1.NODE[int_var].$MOVE_TYPE := LINEAR



		int_var := int_var + 1
		
		pth1.NODE[int_var].$MOVE_TYPE := SEG_VIA



		int_var := int_var + 1



		pth1.NODE[int_var].$MOVE_TYPE := CIRCULAR



	ENDFOR


	FOR int_var := 17 TO 31 DO



		pth1.NODE[int_var].$MOVE_TYPE := LINEAR



		int_var := int_var + 1



		pth1.NODE[int_var].$MOVE_TYPE := SEG_VIA



		int_var := int_var + 1



		pth1.NODE[int_var].$MOVE_TYPE := CIRCULAR



	ENDFOR
	trigger_on:= TRUE
	trigger_off:= FALSE

	WRITE LUN_CRT (NL, '#######Starting trajectory...', NL)
	MOVE ALONG pth1 WITH $SPD_OPT = SPD_LIN 
	CANCEL ALL
	NODE_DEL(pth1, 1, 32)

	trigger_on:= FALSE
	trigger_off:= TRUE
	WRITE LUN_CRT (NL, '#######Motion program YO3 end...', NL)

END YO3